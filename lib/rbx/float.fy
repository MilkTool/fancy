class Float {
  """
  Standard class for floating point number values in Fancy.
  """

  include: Number

  ruby_alias: '+
  ruby_alias: '-
  ruby_alias: '*
  ruby_alias: '/
  ruby_alias: '==
  ruby_alias: '>=
  ruby_alias: '<=
  ruby_alias: '<=>
  ruby_alias: 'to_i
  ruby_alias: 'to_f
  ruby_alias: '**
}
